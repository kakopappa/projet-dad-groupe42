<Window x:Class="TestNavigation.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Button Click="Button_Click" Content="Go to next page" HorizontalAlignment="Left" VerticalAlignment="Top" />
        <Button Click="Button_Click_2" Content="Go to page" HorizontalAlignment="Center" VerticalAlignment="Top" />
        <Button Click="Button_Click_1" Content="Go back" HorizontalAlignment="Right" VerticalAlignment="Top" />
        <Frame Margin="0,50,0,0" JournalOwnership="UsesParentJournal" Name="frame" />
    </Grid>
</Window>