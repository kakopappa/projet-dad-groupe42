<Application x:Class="Client.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources/TextBox.xaml" />
                <ResourceDictionary Source="Resources/Colors.xaml" />
                <ResourceDictionary Source="Resources/Blue.xaml" />
                <ResourceDictionary Source="Resources/Generic.xaml" />
                <ResourceDictionary Source="Resources/Animations.xaml" />
                <ResourceDictionary Source="Resources/Headings.xaml" />
                <ResourceDictionary Source="Resources/Scrollbar.xaml" />
                <ResourceDictionary Source="Resources/Scrollviewer.xaml" />
                <ResourceDictionary Source="Themes/Generic.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>